{
  "name": "hostiq",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "db:generate": "pnpm --filter @hostiq/db generate",
    "db:migrate": "pnpm --filter @hostiq/db migrate",
    "db:push": "pnpm --filter @hostiq/db push",
    "db:seed": "pnpm --filter @hostiq/db seed",
    "db:studio": "pnpm --filter @hostiq/db studio"
  },
  "devDependencies": {
    "playwright": "^1.58.2",
    "puppeteer": "^24.37.3",
    "turbo": "^2"
  },
  "packageManager": "pnpm@9.15.4",
  "engines": {
    "node": ">=20"
  }
}
